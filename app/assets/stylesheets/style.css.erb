}

i{
	margin-right: 5px;
}


.padd{
	padding: 10px;
}

.bold, strong{
	font-weight: 600;
}

a:hover,a:active,a:focus {
outline: 0;
}

:focus {outline:none;}

::-moz-focus-inner {border:0;}



form *{
	font-size: 13px !important;
}

form input, form button, form textarea{
	font-size:12px !important;
	margin:5px 0px;
}

form{
