/* Slide Box*/

.slide-box{
	width: 250px;
	z-index: 400;
	position: fixed;
	top: 35%;
	
	border: 1px solid #000;
	right: -252px;
}

.slide-box .slide-box-button{
	background: #afa3ec url(<%= asset_path"fondo.png" %>) repeat;
	border-right: 0px;
	border-right: 0px;
	text-align: center;
	line-height: 40px;
	width: 29px;
	height: 40px;
	display: block;
	position: absolute;
	z-index: 400;
	left: -30px;
	top: 35%;
	cursor: pointer;
}


.slide-box i{
	margin-right: 2px;
}



