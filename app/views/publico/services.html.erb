<!DOCTYPE html>
<html lang="en">
<head>
 
</head>

<body>

<!-- Main content starts -->

<div class="content">

  <!-- Sidebar starts -->
  <%= render(:partial => 'publico/sidebar')%>
  <!-- Sidebar ends -->

  <!-- Mainbar starts -->
  <div class="mainbar">

    <!-- Contact box starts  slide-->
